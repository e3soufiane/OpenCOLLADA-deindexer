<Window x:Class="colladaDeindexer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button Content="Browse" HorizontalAlignment="Left" Margin="387,66,0,0" VerticalAlignment="Top" Width="75" Click="browseButtonClick" RenderTransformOrigin="0.416,-0.522" Height="23"/>
        <TextBox Name="inputFileField" HorizontalAlignment="Left" Height="23" Margin="84,66,0,0" Text="" VerticalAlignment="Top" Width="268" IsReadOnly="True"/>
        <CheckBox Content="Position" HorizontalAlignment="Left" Margin="84,119,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.014,-0.316" IsEnabled="False" IsChecked="True"/>
        <CheckBox Content="Normal" HorizontalAlignment="Left" Margin="188,119,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.014,-0.316" IsEnabled="False" IsChecked="True"/>
        <CheckBox Content="Texcoord" HorizontalAlignment="Left" Margin="293,119,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.014,-0.316" IsChecked="True" IsEnabled="False"/>
        <CheckBox x:Name="tangentCheckbox" Content="Tangent/Bitangent" HorizontalAlignment="Left" Margin="84,153,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.014,-0.316"/>

    </Grid>
</Window>
